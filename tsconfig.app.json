{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue",
		"@servimav/**/*.ts",
		"node_modules/vite-plugin-pwa/client.d.ts"
	],
	"exclude": [
		"src/**/__tests__/*"
	],
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@servimav/*": [
				"@servimav/*"
			]
		}
	}
}